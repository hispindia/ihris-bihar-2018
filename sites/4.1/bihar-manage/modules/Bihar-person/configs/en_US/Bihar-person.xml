<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Bihar-person" path="/I2CE">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="Bihar-person" path="/I2CE" locale="en_US">
    <version>4.1.0.1</version>
    <configurationGroup name="forms" path="/modules/forms/formClasses">
      <configurationGroup name="iHRIS_Person">
        <configurationGroup name="fields">
          <configurationGroup name="seniority_no">
            <configuration name="headers" type="delimited" locale="en_US">
              <value>default:Seniority No.(according to 2008 list)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="blood_group">
            <configuration name="headers" type="delimited" locale="en_US">
              <value>default:Blood Group</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="birth_mark">
            <configuration name="headers" type="delimited" locale="en_US">
              <value>default:Birth Mark</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>