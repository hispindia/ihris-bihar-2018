<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="India-demographic" path="/I2CE">
  <metadata>
    <version>4.1.0.0</version>
  </metadata>
  <configurationGroup name="India-demographic" path="/I2CE" locale="en_US">
    <version>4.1.0.0</version>
    <configurationGroup name="forms" path="/modules/forms/formClasses">
      <configurationGroup name="iHRIS_Demographic">
        <configurationGroup name="fields">
          <configurationGroup name="birth_location">
            <configuration name="headers" type="delimited" locale="en_US">
              <value>default:Place of Birth</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="birth_location_village">
            <configuration name="headers" type="delimited" locale="en_US">
              <value>default:Place of Birth - Village</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>