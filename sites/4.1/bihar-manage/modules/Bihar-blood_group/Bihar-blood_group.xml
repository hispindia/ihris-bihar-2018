<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Bihar-blood_group">
  <metadata>
    <displayName>Blood Group</displayName>
    <category>Site</category>
    <description>Blood group drop down for mapped fields.</description>
    <version>4.1.8.0</version>
    <path name="configs">
      <value>./configs</value>
    </path>
    <link>https://launchpad.net/indian-ihris</link>
    <requirement name="Lists">
      <atLeast version="4.1" />
      <lessThan version="4.2" />
    </requirement>
    <path name="templates">
      <value>./templates</value>
    </path>
    <path name="classes">
      <value>./lib</value>
    </path>
    <priority>350</priority>
  </metadata>
  <configurationGroup name="Bihar-blood_group" path="/I2CE">

    <configurationGroup name="tasks">
      <configurationGroup name="task_description" locale="en_US">
        <configuration name="can_edit_database_list_blood_group" locale="en_US">
          <value>Can edit the blood_group database list</value>
        </configuration>
        <configuration name="can_view_database_list_blood_group" locale="en_US">
          <value>Can view the blood_group database list</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="task_trickle_down">
      </configurationGroup>
    </configurationGroup>

    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="blood_group">
          <displayName>Blood Group</displayName>
          <description>The Blood Group Form</description>
          <configuration name="class" values="single">
            <displayName>Class Name</displayName>
            <description>The name of the class providing the form</description>
            <value>I2CE_SimpleList</value>
          </configuration>
          <configuration name="display" values="single" locale="en_US">
            <displayName>Display name</displayName>
            <description>The display name for this form</description>
            <value>Blood Group</value>
          </configuration>
          <configuration name="storage" values="single">
            <displayName>Storage Details</displayName>
            <description>The storage mechanism for this form.</description>
            <value>magicdata</value>
          </configuration>
        </configurationGroup> <!-- The Blood Group form -->
      </configurationGroup> <!-- end forms -->
    </configurationGroup> <!-- end /modules/forms -->

    <configurationGroup name="blood_group_data" path="/I2CE/formsData/forms/blood_group">

      <configurationGroup name="apos">
        <displayName>apos</displayName>
        <configuration name="last_modified">
          <value>2013-08-30 12:00:00</value>
        </configuration>
        <configuration name="who">
          <value>1</value>
        </configuration>
        <configuration name="fields" type="delimited">
          <value>name:A +</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="aneg">
        <displayName>aneg</displayName>
        <configuration name="last_modified">
          <value>2013-08-30 12:00:00</value>
        </configuration>
        <configuration name="who">
          <value>1</value>
        </configuration>
        <configuration name="fields" type="delimited">
          <value>name:A -</value>
        </configuration>
      </configurationGroup>

      <configurationGroup name="bpos">
        <displayName>bpos</displayName>
        <configuration name="last_modified">
          <value>2013-08-30 12:00:00</value>
        </configuration>
        <configuration name="who">
          <value>1</value>
        </configuration>
        <configuration name="fields" type="delimited">
          <value>name:B +</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="bneg">
        <displayName>bneg</displayName>
        <configuration name="last_modified">
          <value>2013-08-30 12:00:00</value>
        </configuration>
        <configuration name="who">
          <value>1</value>
        </configuration>
        <configuration name="fields" type="delimited">
          <value>name:B -</value>
        </configuration>
      </configurationGroup>

      <configurationGroup name="abpos">
        <displayName>abpos</displayName>
        <configuration name="last_modified">
          <value>2013-08-30 12:00:00</value>
        </configuration>
        <configuration name="who">
          <value>1</value>
        </configuration>
        <configuration name="fields" type="delimited">
          <value>name:AB +</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="abneg">
        <displayName>abneg</displayName>
        <configuration name="last_modified">
          <value>2013-08-30 12:00:00</value>
        </configuration>
        <configuration name="who">
          <value>1</value>
        </configuration>
        <configuration name="fields" type="delimited">
          <value>name:AB -</value>
        </configuration>
      </configurationGroup>

      <configurationGroup name="opos">
        <displayName>opos</displayName>
        <configuration name="last_modified">
          <value>2013-08-30 12:00:00</value>
        </configuration>
        <configuration name="who">
          <value>1</value>
        </configuration>
        <configuration name="fields" type="delimited">
          <value>name:O +</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="oneg">
        <displayName>oneg</displayName>
        <configuration name="last_modified">
          <value>2013-08-30 12:00:00</value>
        </configuration>
        <configuration name="who">
          <value>1</value>
        </configuration>
        <configuration name="fields" type="delimited">
          <value>name:O -</value>
        </configuration>
      </configurationGroup>

    </configurationGroup>

  </configurationGroup>
</I2CEConfiguration>
